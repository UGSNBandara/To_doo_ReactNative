{
  "expo": {
    "name": "to_doo",
    "slug": "to_doo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assest/icons/app.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assest/icons/app.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sulitha.to_doo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assest/icons/app.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assest/icons/app_start.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b4dc22c6-9f42-42ee-a112-4a4df2d7e7d1"
      }
    }
  }
}
